<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>Recalcine</title>
		<%= favicon_link_tag %>
	<meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
	<%= csrf_meta_tags %>
	<!-- Bootstrap 3.3.4 -->
	<%= stylesheet_link_tag 'bootstrap.min', media: 'all', 'data-turbolinks-track' => true %>
	<!-- FontAwesome 4.3.0 -->
	<%= stylesheet_link_tag 'https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css', media: 'all', 'data-turbolinks-track' => true %>
	<!-- <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" /> -->
	<!-- Ionicons 2.0.0 -->
	<%= stylesheet_link_tag 'https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css', media: 'all', 'data-turbolinks-track' => true %>
	<!-- <link href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css" rel="stylesheet" type="text/css" /> -->
	<!-- Theme Style -->
	<%= stylesheet_link_tag 'AdminLTE.min', media: 'all', 'data-turbolinks-track' => true %>
	<!-- AdminLTE Skins. Choose a skin from the css/skins 
	folder instead of downloading all of them to reduce the load. -->
	<%= stylesheet_link_tag 'skins/_all-skins.min', media: 'all', 'data-turbolinks-track' => true %>
	<!-- iCheck -->
	<%= stylesheet_link_tag 'plugins/iCheck/flat/blue', media: 'all', 'data-turbolinks-track' => true %>
	<!-- Morris chart -->
	<%= stylesheet_link_tag 'plugins/morris/morris', media: 'all', 'data-turbolinks-track' => true %>
	<!-- jvectormap -->
	<%= stylesheet_link_tag 'plugins/jvectormap/jquery-jvectormap-1.2.2', media: 'all', 'data-turbolinks-track' => true %>
	<!-- Date Picker -->
	<%= stylesheet_link_tag 'plugins/datepicker/datepicker3', media: 'all', 'data-turbolinks-track' => true %>
	<!-- Daterange picker -->
	<%= stylesheet_link_tag 'plugins/daterangepicker/daterangepicker-bs3', media: 'all', 'data-turbolinks-track' => true %>
	<!-- bootstrap wysihtml5 - text editor -->
	<%= stylesheet_link_tag 'plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min', media: 'all', 'data-turbolinks-track' =>true %>
	<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
	<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
	<!--[if lt IE 9]>
	    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
	    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
	    <![endif]-->
	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<%#= javascript_include_tag 'bootstrap.min', 'data-turbolinks-track' => true %>
	<%#= javascript_include_tag 'iCheck/icheck.min', 'data-turbolinks-track' => true %>
</head>
<body class="login-page">
	<%= yield %>
</body>
</html>
